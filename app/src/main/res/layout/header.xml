            android:layout_width="300dp"
            android:layout_height="100dp"
            app:srcCompat="@mipmap/logotip2_round"
            tools:layout_editor_absoluteX="2dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>